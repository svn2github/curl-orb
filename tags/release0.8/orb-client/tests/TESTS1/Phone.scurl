
{define-class public serializable Phone 
 
  field private _name:#String 
  field private _number:#String 

  {getter public {name}:#String 
    {return self._name} 
  } 

  {getter public {number}:#String 
    {return self._number} 
  } 

  {setter public {name v:#String}:void 
    set self._name = v 
  } 

  {setter public {number v:#String}:void 
    set self._number = v 
  } 

  {constructor public {default} } 
} 
