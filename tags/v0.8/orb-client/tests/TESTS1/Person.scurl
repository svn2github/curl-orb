
{define-class public serializable Person 
 
  field private _name:#String 
  field private _birthday:double 
  field private _nickname:#String 
  field private _clothes:#{FastArray-of double} 
  field private _main-phone-number:#Phone 
  field private _phone-numbers:#{FastArray-of #Phone} 
  field public transient transient-field:#String 
  field public public-field:#String 

  |#
  {method public {get-xxxx}:#String 
     
  } 

  {method public {set-xxxx v0:#String}:void 
     
  } 
  #|

  {getter public {name}:#String 
    {return self._name} 
  } 

  {getter public {birthday}:double 
    {return self._birthday} 
  } 

  {getter public {nickname}:#String 
    {return self._nickname} 
  } 

  {getter public {clothes}:#{FastArray-of double} 
    {return self._clothes} 
  } 

  {getter public {main-phone-number}:#Phone 
    {return self._main-phone-number} 
  } 

  {getter public {phone-numbers}:#{FastArray-of #Phone} 
    {return self._phone-numbers} 
  } 

  {setter public {name v:#String}:void 
    set self._name = v 
  } 

  {setter public {birthday v:double}:void 
    set self._birthday = v 
  } 

  {setter public {phone-numbers v:#{FastArray-of #Phone}}:void 
    set self._phone-numbers = v 
  } 

  {setter public {nickname v:#String}:void 
    set self._nickname = v 
  } 

  {setter public {clothes v:#{FastArray-of double}}:void 
    set self._clothes = v 
  } 

  {setter public {main-phone-number v:#Phone}:void 
    set self._main-phone-number = v 
  } 

  {constructor public {default} }
} 
